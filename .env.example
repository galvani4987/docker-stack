# Credenciais do Banco de Dados PostgreSQL
# Substitua a senha abaixo por uma senha forte e única no seu arquivo .env
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=<CHANGE_THIS_TO_A_STRONG_PASSWORD>

# Segredos do Authelia (serão adicionados na Fase 3)
# AUTHELIA_JWT_SECRET=<CHANGE_THIS_TO_A_RANDOM_STRING>
# NOTIFIER_SMTP_PASSWORD=<YOUR_SMTP_PASSWORD>
# SESSION_SECRET=<CHANGE_THIS_TO_A_RANDOM_STRING>

# Token do DuckDNS para o Caddy (será adicionado na Fase 1)
# DUCKDNS_TOKEN=<YOUR_DUCKDNS_TOKEN>
